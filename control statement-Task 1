package org.controlstatement;
public class ControlStatement {
	private void main(int a) {
		if(a%2==0) {
			System.out.println("even");
		}
		else {
			System.out.println("odd");
		}
	}
	public static void main(String[]args) {
		ControlStatement a=new ControlStatement();
		a.main(18);
 }
}
Output:
Even
